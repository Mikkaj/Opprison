options:
	Sell: &8[&eSell&8]&7


on rightclick:
	if player is sneaking:
		if "%player's tool%" contain "diamond_pickaxe", "iron_pickaxe", "golden_pickaxe", "wooden_pickaxe" or "stone_pickaxe":
			make player execute command "/sell sneak+click"


command /autosell [<text>]:
	trigger:
		if {Autosell.%player%} is true:
			message "&8[&3AutoSell&8] &7Du har &cdeaktiveret &7autosell!"
			set {Autosell.%player%} to false
		else if {Autosell.%player%} is false:
			message "&8[&3AutoSell&8] &7Du har &aaktiveret &7autosell!"
			set {Autosell.%player%} to true

		else if {Autosell.%player%} is not set:
			set {Autosell.%player%} to true
			message "&8[&3AutoSell&8] &7Du har &aaktiveret &7autosell!"

on block break:
	if "%player's tool%" contain "diamond_pickaxe", "iron_pickaxe", "golden_pickaxe", "wooden_pickaxe" or "stone_pickaxe":
		add 1 to {Block.%player%}


command /blocks:
	trigger:
		message "&8[&3Server&8] &7Du har mined &f%{Block.%player%} + 0% &7blocks!"

on block break:
	if "%player's tool%" contain "diamond_pickaxe", "iron_pickaxe", "golden_pickaxe", "wooden_pickaxe" or "stone_pickaxe":
		if {Autosell.%player%} is true:
			add 1 to {blocks.autosell.%player%}
			if {blocks.autosell.%player%} is bigger than 40:
				set {blocks.autosell.%player%} to 0
				make player execute command "/sell"
		


