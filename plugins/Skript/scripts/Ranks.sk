		if arg 1 is not set:
			send "&8Brug &a/buycrafts spiller rank"
			send "&a"
			send "&cRanks &7/ &cKits:"
			send "&aCoal, Iron, Gold, Lapis, Redstone , Diamond , Emerald , Obsidian , Bedrock, Admin , Yt , Mod , Byg"
		if arg 1 is set:
			if arg 2 is "Coal":
				log "%player% gav %arg 1% %arg 2% rank" to "Ranks/donor/give"
				make player execute command "manuaddsub %arg 1% Coal"
				make player execute command "manuaddv %arg 1% suffix &8[&8Coal&8]&f"
				make player execute command "nte player %arg-1% &8Coal &7| &f"
				broadcast "{@Butik} &b%arg 1% &7har købt &8Coal&7 ranken!"
				broadcast ""
				wait 1.5 ticks
				broadcast "{@Butik} En tilfældig spiller, vil nu få &f£15 Shards&7!"
				broadcast ""
				set {_Vinder} to random player out of all players
				wait 1.5 seconds
				broadcast "{@Butik} Vinderen af &f£15 Shards &7er &b%{_Vinder}%&7!"
				add 15 to {%{_Vinder}%.Tokens}
